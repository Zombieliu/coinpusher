{
  "name": "tsrpc",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/mocha": "^8.2.3",
    "@types/msgpack-lite": "^0.1.12",
    "@types/node": "^15.14.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^30.2.0",
    "mocha": "^9.2.2",
    "onchange": "^7.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "tsrpc-cli": "^2.4.5",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@dimforge/rapier3d-compat": "^0.19.3",
    "@types/ws": "^8.18.1",
    "ioredis": "^5.8.2",
    "mongodb": "^7.0.0",
    "msgpack-lite": "^0.1.26",
    "pm2": "^5.2.0",
    "prom-client": "^15.1.0",
    "qrcode": "^1.5.3",
    "redis": "^4.7.1",
    "seedrandom": "^3.0.5",
    "speakeasy": "^2.0.0",
    "tsrpc": "^3.4.5",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:robot": "tsrpc-cli dev --entry src/ServerRobot.ts",
    "dev:gate": "tsrpc-cli dev --entry src/ServerGate.ts",
    "dev:match": "tsrpc-cli dev --entry src/ServerMatch.ts",
    "dev:room": "tsrpc-cli dev --entry src/ServerRoom.ts",
    "dev:room2": "cross-env PORT=3002 tsrpc-cli dev --entry src/ServerRoom.ts",
    "build": "tsrpc-cli build",
    "doc": "tsrpc-cli doc",
    "proto": "tsrpc-cli proto",
    "sync": "tsrpc-cli sync",
    "api": "tsrpc-cli api",
    "test": "mocha",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "doc:generate": "./scripts/generate-api-docs.sh",
    "quality": "./scripts/format-code.sh check"
  }
}
